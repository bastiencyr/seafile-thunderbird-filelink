{
    "manifest_version": 2,
    "name": "Seafile Filelink2",
    "browser_specific_settings": {
      "gecko": {
        "id":  "seafilecloud@bastien-coco.fr"
      }
    },
    "description": "store attachments on a seafile share",
    "version": "0.0.5",
    "author": "Bastien Coco",
    "icons": {
        "64": "images/icon-64px.png",
        "32": "images/icon-32px.png",
        "16": "images/icon-16px.png"
    },
    "background": {
        "page": "background.html"
    },
    "cloud_file": {
      "name": "Seafile",
      "management_url": "management.html"
    },
    "permissions": [
      "storage", "*://*/*"
    ]
}
